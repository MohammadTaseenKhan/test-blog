{
   "version": 2,
   "scripts": {
      "start": "node index.js"
    },
   "name": "replit-docs",
   "builds": [
     { "src": "index.js", "use": "@vercel/node" }
   ],
   "routes": [
     { "handle": "filesystem" },
     {
       "src": "/index.js",
       "dest": "/index.js"
     },
     {
       "src": "/.*",
       "dest": "/index.js"
     }
   ]
 }
 